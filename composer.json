{
  "name": "mbvb1223/pinecone-php-client",
  "description": "A PHP SDK for Pinecone vector database",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "pinecone",
    "vector",
    "database",
    "embeddings",
    "similarity",
    "search",
    "ai",
    "ml"
  ],
  "authors": [
    {
      "name": "Khien",
      "email": "phamkhien2309@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.90",
    "mockery/mockery": "^1.5",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Mbvb1223\\Pinecone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mbvb1223\\Pinecone\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix": "vendor/bin/php-cs-fixer fix . -vvv",
    "check": "vendor/bin/php-cs-fixer check . -vvv",
    "test": "vendor/bin/phpunit --testsuite 'Unit Tests' --testdox",
    "test:integration": "vendor/bin/phpunit -c phpunit-integration.xml --testdox",
    "test:all": "vendor/bin/phpunit --testdox",
    "phpstan": "vendor/bin/phpstan analyse"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
