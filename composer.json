{
  "name": "mbvb1223/pinecone-php-client",
  "description": "A PHP SDK for Pinecone vector database",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "pinecone",
    "vector",
    "database",
    "embeddings",
    "similarity",
    "search",
    "ai",
    "ml"
  ],
  "authors": [
    {
      "name": "Khien",
      "email": "phamkhien2309@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "cuyz/valinor": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7",
    "php-cs-fixer/shim": "^3.0",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Mbvb1223\\Pinecone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mbvb1223\\Pinecone\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix": "vendor/bin/php-cs-fixer fix . -vvv",
    "check": "vendor/bin/php-cs-fixer check . -vvv",
    "test": "vendor/bin/phpunit --testdox"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
